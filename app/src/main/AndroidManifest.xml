<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.paulohpfranco.musicalstructureapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".QueenPlaylistActivity"
            android:label="@string/mainactivity_list_queen"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity
            android:name=".TobyMacPlaylistActivity"
            android:label="@string/mainactivity_list_tobymac"
            android:parentActivityName=".MainActivity">
        </activity>
    </application>

</manifest>